<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
  <class dynamic-insert="false" dynamic-update="false" mutable="true" name="DEL.DailyAmounts" optimistic-lock="version" polymorphism="implicit" select-before-update="false">
    <id column="id" name="id" type="long">
      <generator class="identity"/>
    </id>
    <property column="MaxDailyCarbUnit" name="maxCarbUnitsPerDay" type="int"/>
    <property column="MaxDailtProteinUnit" name="maxProteinUnitsPerDay" type="int"/>
    <property column="MaxDailyFatUnit" name="maxFatUnitsPerDay" type="int"/>
  </class>
</hibernate-mapping>
